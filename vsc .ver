import discord
import asyncio

client =discord.Client()

token =
@client.event
async def on_message(message):
    if message.content == '주사위':
        import random
        dice = random.randint(1, 6)
        embed = discord.Embed(title='{}' .format(dice), description='주사위를 굴려 {}가 나왔습니다!' .format(dice))
        await message.channel.send (embed=embed)

